{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    ["import", { "libraryName": "lodash" }, "lodash"],
    [
      "import",
      { "libraryName": "antd-mobile", "libraryDirectory": "lib" },
      "antd-mobile"
    ]
  ]
}
